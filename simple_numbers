def gcd(a, b):
  """
  Функция для вычисления НОД двух чисел.

  Args:
    a: Первое число.
    b: Второе число.

  Returns:
    НОД двух чисел.
  """

  while b:
    a, b = b, a % b
  return a


def are_coprime(a, b):
  """
  Функция для определения, являются ли два числа взаимно простыми.

  Args:
    a: Первое число.
    b: Второе число.

  Returns:
    True, если числа взаимно просты, False - в противном случае.
  """

  return gcd(a, b) == 1


# Ввод чисел
a = int(input("Введите первое число: "))
b = int(input("Введите второе число: "))

# Определение взаимной простоты
if are_coprime(a, b):
  print(f"Числа {a} и {b} взаимно просты.")
else:
  print(f"Числа {a} и {b} не являются взаимно простыми.")
